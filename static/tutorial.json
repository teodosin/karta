      "modifiedAt": 1746217274351,
      "path": "/Welcome To Karta",
      "attributes": {
        "name": "Welcome To Karta",
        "text": "Pan the canvas with middle-mouse dragging or a touchpad. Zoom with the mouse wheel or by pinching.\n\nTo learn how to use this app, look down to see the node called \"Guide - Hub\", then right click and select \"Enter Context\" to be taken to the guide.\n",
        "karta_fontSize": 16,
        "karta_fillColor": "rgba(255, 250, 196, 0)",
        "karta_textColor": "rgba(190, 178, 178, 1)",
        "karta_font": "Nunito",
        "karta_isNameVisible": false
      },
      "isSearchable": false
    },
