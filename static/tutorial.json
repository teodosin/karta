    {
      "id": "7256c99f-93bf-4675-8078-7393b49ac640",
      "ntype": "text",
      "createdAt": 1746215363735,
      "modifiedAt": 1746218233905,
      "path": "/Guide - View Navigation",
      "attributes": {
        "name": "Guide - View Navigation",
        "text": "To navigate within a context:\n\n* Panning - drag with middle-mouse pressed, or two-finger drag with a touchpad.\n* Zooming - use scroll wheel, or two-finger pinch with a touchpad. \n* Center the focal node - from the background's right-click menu, or by pressing \"F\"\n* Frame the current view contents - from the background's right-click menu, or by pressing Shift + \"F\"",
        "karta_fontSize": 12,
        "karta_fillColor": "rgba(94, 94, 85, 0.07)",
        "karta_textColor": "rgba(221, 221, 221, 1)",
        "karta_font": "Nunito",
        "karta_isNameVisible": false
      },
      "isSearchable": true
    },
    {
      "id": "814242b1-9a2f-46dd-8abd-07493c7a1734",
      "ntype": "text",
      "createdAt": 1746215777718,
      "modifiedAt": 1746217813960,
      "path": "/Guide - Navigating Between Contexts",
      "attributes": {
        "name": "Guide - Navigating Between Contexts",
        "text": "Navigating between contexts:\n\nRight-click on a node and press \"Enter Context\" to see the node network from that node's point of view.\n\nYou can also use the Context Tool from the Tool Bar on the left of the screen to make all node clicks change between contexts.",
        "karta_fontSize": 12,
        "karta_fillColor": "rgba(255, 250, 196, 0)",
        "karta_textColor": "rgba(221, 221, 221, 1)",
        "karta_font": "Nunito",
        "karta_isNameVisible": false
      },
      "isSearchable": true
    },
